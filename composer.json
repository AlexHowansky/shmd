{
    "name": "shmd/gallery",
    "type": "project",
    "description": "South High Marathon Dance photo ordering application.",
    "homepage": "http://github.com/AlexHowansky/shmd",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Howansky",
            "email": "alex.howansky@gmail.com",
            "homepage": "https://github.com/AlexHowansky"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "aws/aws-sdk-php": "^3.371.3",
        "bacon/bacon-qr-code": "^3.0.3",
        "mike42/escpos-php": "^4.0",
        "ork/csv": "^2.3.0"
    },
    "require-dev": {
        "ork/phpcs": "^3.0.0",
        "phpstan/phpstan": "^2.1.40",
        "rector/rector": "^2.3.8"
    },
    "autoload": {
        "psr-4": {
            "Shmd\\": "src/"
        }
    },
    "scripts": {
        "go": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:8000 -t public/"
        ],
        "phpcs": "phpcs",
        "phpstan": "phpstan analyze",
        "rector": "rector --dry-run --clear-cache"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
