{
    "name": "shmd/gallery",
    "type": "project",
    "description": "South High Marathon Dance photo ordering application.",
    "homepage": "http://github.com/AlexHowansky/shmd",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Howansky",
            "email": "alex.howansky@gmail.com",
            "homepage": "https://github.com/AlexHowansky"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "aws/aws-sdk-php": "^3.218.0",
        "mike42/escpos-php": "^2.2",
        "ork/csv": "^1.5.1"
    },
    "require-dev": {
        "ork/phpcs": "^2.7.0"
    },
    "autoload": {
        "psr-4": {
            "Shmd\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "post-install-cmd": "phpcs --config-set php_version 70331",
        "post-update-cmd": "phpcs --config-set php_version 70331",
        "go": "php -S localhost:8000 -t public/"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
